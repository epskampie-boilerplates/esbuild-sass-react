    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "esbuild-plugin-sass": "^0.3.3",
    "mobx": "^6.1.8",
    "mobx-react": "^7.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
